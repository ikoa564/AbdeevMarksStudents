<Window x:Class="AbdeevMarksStudents.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AbdeevMarksStudents"
        mc:Ignorable="d"
        Title="Успеваемость студентов ВУЗа" Height="750" Width="1100" MinHeight="750" MinWidth="1100" Icon="/res/uunt.jpg" FontFamily="Calibri" >
    <Grid Background="#FF843D">
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>

        <Image Grid.Row="0" Source="/res/uunt.jpg" HorizontalAlignment="Left" VerticalAlignment="Center"></Image>
        <TextBlock Text="Успеваемость студентов ВУЗа УУНиТ" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Button HorizontalAlignment="Right" VerticalAlignment="Center" Name="BackBtn" Click="BackBtn_Click" Content="Назад"></Button>

        <Frame Background="White" Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
